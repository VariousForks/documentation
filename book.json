{
  "gitbook": "3.2.2",
  "title": "TagSpaces Documentation",
  "author": "TagSpaces UG (haftungbeschraenkt)",
  "plugins": [
    "edit-link",
    "anchorjs",
    "prism",
    "-highlight",
    "toc",
    "sitemap",
    "ga",
    "expandable-chapters"
  ],
  "search": {
    "maxIndexSize": 1000000000
  },
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/tagspaces/documentation/edit/master",
      "label": "Edit"
    },
    "ga": {
      "token": "UA-90842098-1",
      "configuration": {
        "anonymizeIp": true
      }
    },
    "sitemap": {
        "hostname": "http://docs.tagspaces.org/"
    }
  }
}