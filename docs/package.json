{
  "name": "tagspaces-documentation",
  "version": "1.0.0",
  "description": "Documentation for the TagSpaces application",
  "main": "index.js",
  "dependencies": {
    "gitbook-plugin-edit-link": "^2.0.2"
  },
  "devDependencies": {},
  "scripts": {
